{
  "extends": ["@v1/typescript/base.json"],
  "compilerOptions": {
    "incremental": true,
    "tsBuildInfoFile": ".tsbuildinfo",
    "noImplicitAny": false,
    "jsx": "preserve",
    "moduleResolution": "Bundler",
    "exactOptionalPropertyTypes": false
  },
  "include": ["**/*.ts", "**/*.js"]
}
